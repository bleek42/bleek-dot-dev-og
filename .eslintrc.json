{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": [
		"eslint:recommended",
		"plugin:react/recommended",
		"prettier"
	],
	"overrides": [],
	"parser": "babel",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": [
		"react",
		"prettier"
	],
	"rules": {
		"react/react-in-jsx-scope": "off",
		"prettier/prettier": "warn",
		"camelcase": "warn",
		"quotes": ["warn", "single"]
	}
}
